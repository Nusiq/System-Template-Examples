{
	"format_version": "1.8.0",
	"animations": {
		"animation.snail.walk": {
			"loop": true,
			"anim_time_update": "q.modified_distance_moved*0.3",
			"animation_length": 1,
			"bones": {
				"body": {
					"scale": [1, 1, "math.sin(q.anim_time*360)*0.1+1"]
				},
				"shell": {
					"position": [0, "-math.sin(q.anim_time*360)*0.1", 0]
				}
			}
		}
	}
}
